#!/usr/bin/env php
<?php

if (file_exists('vendor')) {
    include __DIR__ . DIRECTORY_SEPARATOR . 'vendor' . DIRECTORY_SEPARATOR . "autoload.php";
} else {
    include dirname(dirname(__DIR__)) . DIRECTORY_SEPARATOR . "autoload.php";
}

require __DIR__ .
    DIRECTORY_SEPARATOR . "PHP" .
    DIRECTORY_SEPARATOR . "ParserGenerator" .
    DIRECTORY_SEPARATOR . "cli.php";
